<?xml version="1.0" encoding="UTF-8"?>
<excel>
   <entity name="商品" code="product" >
   	
		<column name="标题" code="title" type="String" property="title">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="小标题" code="small_title" type="String" property="smallTitle">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="商品名" code="name" type="String" property="name">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="一级分类" code="category_first" type="String" property="categoryFirst">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="二级分类" code="category_second" type="String" property="categorySecond">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="价格" code="price" type="String" property="price">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="商品编号" code="num" type="String" property="num">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="库存" code="stock" type="String" property="stock"></column>
		<column name="重量" code="weight" type="String" property="weight"></column>
		<column name="入库时间" code="stock_time" type="String" property="stockTime">
			<rules>
				<rule code="nullable" message="入库时间不允许为空"></rule>
				<rule code="checkDateFormat" message="入库时间格式错误"></rule>
			</rules>	
		</column>
		<column name="上下状态" code="status" type="String" property="status">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="销量" code="sales" type="String" property="sales"></column>
		<column name="评论数" code="comments" type="String" property="comments"></column>
		<column name="主图1" code="picture1" type="String" property="picture1">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="主图2" code="picture2" type="String" property="picture2"></column>
		<column name="主图3" code="picture3" type="String" property="picture3"></column>
		<column name="主图4" code="picture4" type="String" property="picture4"></column>
		<column name="主图" code="picture" type="String" property="picture">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="热卖标记" code="hot_sell" type="String" property="hotSell"></column>
		<column name="促销标记" code="is_sale" type="String" property="isSale"></column>
		<column name="企业" code="business_id" type="String" property="businessId">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="品牌" code="brand_id" type="String" property="brandId">
			<rules>
				<rule code="nullable" message="不允许为空"></rule>
			</rules>	
		</column>
		<column name="区" code="area" type="String" property="area"></column>
		<column name="市" code="city" type="String" property="city"></column>
		<column name="价格2" code="price2" type="String" property="price2"></column>
		
   </entity>
</excel>

